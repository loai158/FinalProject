@using FinalProject.Core.Feature.Nurse.Query.Response
@model NurseResponse

@{
    ViewBag.Title = "Nurse Details";
}

<h2>Nurse Details</h2>

<table class="table table-striped">
    <tr>
        <th>ID</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Phone Number</th>
        <td>@Model.Phone</td>
    </tr>
    <tr>
        <th>Department</th>
        <td>@Model.DepartmentId</td>
    </tr>
     
   
</table>

<a href="@Url.Action("Index", "Nurse") class="btn btn-secondary">Back to List</a>
